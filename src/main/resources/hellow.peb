<h1>Hello {{ name ? name : "world" }}!</h1>
Stack:
<ul>
    {% for item in stack %}
        <li>{{ item }}</li>
    {% endfor %}
</ul>
<form action="/" method="post">
    <label for="name">Name: </label>
    <input id="name" name="name" value="{{ name ? name : "John Doe" }}" />
    <button type="submit">Submit</button>
</form>
